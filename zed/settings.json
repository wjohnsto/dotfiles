{
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-flash-preview-04-17"
    },
    "version": "2"
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": true,
  "vim_mode": false,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "JetBrainsMonoNL Nerd Font",
  "buffer_font_fallbacks": ["FiraMono Nerd Font Mono"],
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "MyTheme Dark"
  },
  "autosave": "on_focus_change",
  "format_on_save": "off",
  "icon_theme": {
    "mode": "system",
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Frapp√©"
